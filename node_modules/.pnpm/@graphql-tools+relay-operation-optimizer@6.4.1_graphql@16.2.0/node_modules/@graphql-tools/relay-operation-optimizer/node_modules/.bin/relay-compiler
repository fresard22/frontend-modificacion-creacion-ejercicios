#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/relay-compiler@12.0.0_graphql@16.2.0/node_modules/relay-compiler/bin/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/relay-compiler@12.0.0_graphql@16.2.0/node_modules/relay-compiler/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/relay-compiler@12.0.0_graphql@16.2.0/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/relay-compiler@12.0.0_graphql@16.2.0/node_modules/relay-compiler/bin/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/relay-compiler@12.0.0_graphql@16.2.0/node_modules/relay-compiler/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/relay-compiler@12.0.0_graphql@16.2.0/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../relay-compiler@12.0.0_graphql@16.2.0/node_modules/relay-compiler/bin/relay-compiler" "$@"
else
  exec node  "$basedir/../../../../../../relay-compiler@12.0.0_graphql@16.2.0/node_modules/relay-compiler/bin/relay-compiler" "$@"
fi
