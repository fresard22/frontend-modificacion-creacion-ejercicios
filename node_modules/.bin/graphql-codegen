#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/@graphql-codegen+cli@2.4.0_@types+node@16.11.21_graphql@16.2.0_typescript@4.5.5/node_modules/@graphql-codegen/cli/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/@graphql-codegen+cli@2.4.0_@types+node@16.11.21_graphql@16.2.0_typescript@4.5.5/node_modules/@graphql-codegen/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/@graphql-codegen+cli@2.4.0_@types+node@16.11.21_graphql@16.2.0_typescript@4.5.5/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/@graphql-codegen+cli@2.4.0_@types+node@16.11.21_graphql@16.2.0_typescript@4.5.5/node_modules/@graphql-codegen/cli/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/@graphql-codegen+cli@2.4.0_@types+node@16.11.21_graphql@16.2.0_typescript@4.5.5/node_modules/@graphql-codegen/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/@graphql-codegen+cli@2.4.0_@types+node@16.11.21_graphql@16.2.0_typescript@4.5.5/node_modules:/home/fabrizio/Escritorio/TutorIntegrado-integracion2./TutorIntegrado-integracion2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/bin.js" "$@"
fi
